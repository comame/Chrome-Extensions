{
    "manifest_version": 3,
    "name": "Waseda Authentication Better Form",
    "version": "1",

    "host_permissions": [
        "https://iaidp.ia.waseda.jp/idp/profile/SAML2/*"
    ],

    "content_scripts": [{
        "matches": ["https://iaidp.ia.waseda.jp/idp/profile/SAML2/*"],
        "js": ["script.js"],
        "run_at": "document_start"
    }]
}
